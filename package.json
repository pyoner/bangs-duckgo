{
  "type": "module",
  "version": "1.0.0",
  "name": "bangs-duckgo",
  "description": "A library to work with DuckDuckGo bangs that provides utilities.",
  "scripts": {
    "format": "prettier --write src/",
    "fetch": "bun run src/fetch.ts",
    "types": "bunx quicktype -t Raw --readonly --prefer-unions --prefer-types --just-types -s json -o src/types/gen.ts src/bangs.json"
  },
  "module": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/types/index.ts"
    },
    "./bangs.json": {
      "import": "./src/bangs.json"
    }
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [
    "duckduckgo",
    "bangs"
  ],
  "homepage": "https://github.com/pyoner/bangs-duckgo",
  "bugs": {
    "url": "https://github.com/pyoner/bangs-duckgo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pyoner/bangs-duckgo.git"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.3.3",
    "quicktype": "^23.0.170",
    "tsup": "^8.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
