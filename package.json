{
  "name": "bangs-duckgo",
  "module": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./bangs.json": {
      "import": "./src/bangs.json"
    }
  },
  "scripts": {
    "fetch": "bun run src/fetch.ts",
    "types": "bunx quicktype -t Raw --readonly --prefer-unions --prefer-types --just-types -s json -o src/types/gen.ts src/bangs.json"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "quicktype": "^23.0.170"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
